{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "es5",
    "importHelpers": true,
    "strict": true,
    "noEmitHelpers": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "es2015"
    ]
  }
}